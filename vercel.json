{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.csv", "use": "@vercel/static" },
    { "src": "*.jpeg", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(login|logout|config|correios)", "dest": "/api/$1.js" },
    { "src": "/(.*\\..*)", "dest": "/$1" },
    { "src": "/((?!api|.*\\.).*)", "dest": "/index.html" }
  ]
}
